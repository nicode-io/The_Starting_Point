 getData(\"http://localhost:3001/bookables\")\n            .then(bookables => dispatch({\n                type: \"FETCH_BOOKABLES_SUCCESS\",\n                payload: bookables\n            }))\n            .catch(error => dispatch({\n                type: \"FETCH_BOOKABLES_ERROR\",\n                payload: error\n            }));\n    }, []);\n\n    function changeGroup( e ) {\n        dispatch({\n            type: \"SET_GROUP\",\n            payload: e.target.value\n        });\n    }\n\n    function changeBookable( selectedIndex ) {\n        dispatch({\n            type: \"SET_BOOKABLE\",\n            payload: selectedIndex\n        });\n        nextButtonRef.current.focus();\n    }\n\n    function nextBookable() {\n        dispatch({\n            type: \"NEXT_BOOKABLE\",\n            payload: false\n        });\n    }\n\n    function toggleDetails() {\n        dispatch({type: \"TOGGLE_HAS_DETAILS\"});\n    }\n\n    // UI on error catch\n    if (error) {\n        return <p>{error.message}</p>\n    }\n\n    // UI while loading\n    if (isLoading) {\n        return <p><Spinner/> Loading bookables...</p>\n    }\n\n    return (\n        <Fragment>\n            <div>\n                <select\n                    value={group}\n                    onChange={changeGroup}\n                >\n                    {groups.map(g => <option value={g} key={g}>{g}</option>)}\n                </select>\n\n                <ul className=\"bookables items-list-nav\">\n                    {bookablesInGroup.map(( b, i ) => (\n                        <li\n                            key={b.id}\n                            className={i === bookableIndex ? \"selected\" : null}\n                        >\n                            <button\n                                className=\"btn\"\n                                onClick={() => changeBookable(i)}\n                            >\n                                {b.title}\n                            </button>\n                        </li>\n                    ))}\n                </ul>\n                <p>\n                    <button\n                        className=\"btn\"\n                        onClick={nextBookable}\n                        ref={nextButtonRef}\n                        autoFocus\n                    >\n                        <FaArrowRight/>\n                        <span>Next</span>\n                    </button>\n                </p>\n            </div>\n\n            {bookable && (\n                <div className=\"bookable-details\">\n                    <div className=\"item\">\n                        <div className=\"item-header\">\n                            <h2>\n                                {bookable.title}\n                            </h2>\n                            <span className=\"controls\">\n                                <label>\n                                <input\n                                    type=\"checkbox\"\n                                    checked={hasDetails}\n                                    onChange={toggleDetails}\n                                />\n                                Show Details\n                                </label>\n                            </span>\n                        </div>\n\n                        <p>{bookable.notes}</p>\n\n                        {hasDetails && (\n                            <div className=\"item-details\">\n                                <h3>Availability</h3>\n                                <div className=\"bookable-availability\">\n                                    <ul>\n                                        {bookable.days\n                                            .sort()\n                                            .map(d => <li key={d}>{days[d]}</li>)\n                                        }\n                                    </ul>\n                                    <ul>\n                                        {bookable.sessions\n                                            .map(s => <li key={s}>{sessions[s]}</li>)\n                                        }\n                                    </ul>\n                                </div>\n                            </div>\n                        )}\n                    </div>\n                </div>\n            )}\n        </Fragment>\n    );\n}\n","/Users/nicolasdenoel/Nicode-IO/Bookings_Manager_App/front/src/components/Users/UsersList.js",[],"/Users/nicolasdenoel/Nicode-IO/Bookings_Manager_App/front/src/components/Bookables/reducer.js",[],["64","65"],"/Users/nicolasdenoel/Nicode-IO/Bookings_Manager_App/front/src/components/Bookings/WeekPicker.js",[],"/Users/nicolasdenoel/Nicode-IO/Bookings_Manager_App/front/src/components/Bookings/weekReducer.js",[],"/Users/nicolasdenoel/Nicode-IO/Bookings_Manager_App/front/src/utils/date-wrangler.js",[],"/Users/nicolasdenoel/Nicode-IO/Bookings_Manager_App/front/src/utils/api.js",[],"/Users/nicolasdenoel/Nicode-IO/Bookings_Manager_App/front/src/components/UI/Spinner.js",[],{"ruleId":"66","replacedBy":"67"},{"ruleId":"68","replacedBy":"69"},{"ruleId":"70","severity":1,"message":"71","line":21,"column":75,"nodeType":"72","messageId":"73","endLine":21,"endColumn":87},{"ruleId":"66","replacedBy":"74"},{"ruleId":"68","replacedBy":"75"},"no-native-reassign",["76"],"no-negated-in-lhs",["77"],"no-unused-vars","'isPresenting' is assigned a value but never used.","Identifier","unusedVar",["76"],["77"],"no-global-assign","no-unsafe-negation"]